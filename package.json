{
  "name": "t-packer",
  "version": "0.0.1",
  "private": false,
  "description": "TypeScript npm 打包工具",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "t-packer": "./src/bin/index.ts"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.js",
      "require": "./src/index.js"
    }
  },
  "publishConfig": {
    "types": "./src/index.ts",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "bin": {
      "t-packer": "./dist/bin/cjs/index.js"
    },
    "exports": {
      ".": {
        "types": "./src/index.ts",
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE",
    "pnpm-lock.yaml",
    "docs"
  ],
  "scripts": {
    "build": "node -r rts.js/register scripts/build.ts",
    "lint": "biome check ./src",
    "lint:fix": "biome check ./src --write",
    "format": "biome format --write ./src",
    "changeset:version": "pnpm changeset version",
    "changeset:publish": "pnpm changeset publish"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.14.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-found/t-packer.git"
  },
  "bugs": {
    "url": "https://github.com/code-found/t-packer/issues"
  },
  "homepage": "https://github.com/code-found/t-packer#readme",
  "keywords": [
    "typescript",
    "npm",
    "pack",
    "tool"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@changesets/cli": "^2.29.5",
    "@types/node": "^24.2.1",
    "rts.js": "^0.0.9",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@swc/core": "^1.13.3",
    "commander": "^14.0.0",
    "install": "^0.13.0",
    "pnpm": "^10.14.0"
  }
}
